{
  "name": "gcf-graphql",
  "version": "0.1.0",
  "description": "GraphQL as a Google Cloud Function",
  "private": true,
  "main": "index.js",
  "scripts": {
      "dev": "nodemon -exec babel-node dev.js",
      "build": "babel src --out-dir dist",
      "prestart": "npm run build",
      "start": "node dist/index.js",
      "deploy": "npm run build && cd dist && gcloud alpha functions deploy helloGQL --bucket gcf_staging_bucket --trigger-http"
  },
  "license": "MIT",
  "devDependencies": {
    "graphql": "^0.4.14",
    "graphql-relay": "^0.3.6",
    "elasticsearch-graphql": "2.0.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "nodemon": "^1.9.2"
  }
}
